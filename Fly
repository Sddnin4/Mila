    _0xNotifT.TextColor3 = Color3.fromRGB(255, 255, 255)
    _0xNotifT.TextSize = 14
    
    local _0xSlideIn = _0x4d5e6f:Create(_0xNotif, TweenInfo.new(0.3), {Position = UDim2.new(0.5, -150, 0, 20)})
    _0xSlideIn:Play()
    
    spawn(function()
        wait(2)
        local _0xSlideOut = _0x4d5e6f:Create(_0xNotif, TweenInfo.new(0.3), {Position = UDim2.new(0.5, -150, 0, -50)})
        _0xSlideOut:Play()
