-- Encrypted password
local _0xSecKey = {72, 106, 107, 56, 100, 108} -- Hjk8dl in ASCII
local _0xVerify = function(_0xInput)
    local _0xCheck = {}
    for i = 1, #_0xInput do
        _0xCheck[i] = string.byte(_0xInput, i)
    end
    if #_0xCheck ~= #_0xSecKey then return false end
    for i = 1, #_0xSecKey do
        if _0xCheck[i] ~= _0xSecKey[i] then return false end
    end
    return true
end

-- Anti-tamper check
local _0xTamperCheck = function()
    local _0xHash = 0
    for i = 1, #_0xSecKey do
        _0xHash = _0xHash + _0xSecKey[i] * i
    end
    return _0xHash == 2140
end

if not _0xTamperCheck() then
    return error("Security violation detected!")
end

