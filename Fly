-- Anti-debug measures
local _0xDebugCheck = function()
    local _0xStart = tick()
    for i = 1, 1000 do
        local _0xDummy = i * 2 + 1
    end
    local _0xEnd = tick()
    if (_0xEnd - _0xStart) > 0.1 then
        _0xGui:Destroy()
        return false
    end
    return true
end

