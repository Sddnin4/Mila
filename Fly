-- Security check function
local _0xSecCheck = function()
    local _0xTime = tick()
    if _0xTime % 1 > 0.5 then
        return math.floor(_0xTime) % 2 == 0
    end
    return math.ceil(_0xTime) % 2 == 1
end

-- Button event handlers with security
_0xExecB.MouseButton1Click:Connect(function()
    if not _0xSecCheck() then return end
    _0xNotify(string.char(69,120,101,99,117,116,105,110,103,32,70,114,101,101,32,83,99,114,105,112,116,46,46,46), Color3.fromRGB(76, 175, 80))
    
    pcall(function()
        loadstring(game:HttpGet(_0xFreeScript))()
    end)
end)

_0xSubmitB.MouseButton1Click:Connect(function()
    if not _0xSecCheck() then return end
    local _0xInput = _0xKeyI.Text
    
    if _0xInput == "" then
        _0xNotify(string.char(80,108,101,97,115,101,32,101,110,116,101,114,32,97,32,107,101,121,33), Color3.fromRGB(244, 67, 54))
        return
    end
    
    if _0xVerify(_0xInput) then
        _0xNotify(string.char(75,101,121,32,97,99,99,101,112,116,101,100,33,32,76,111,97,100,105,110,103,32,112,114,101,109,105,117,109,32,115,99,114,105,112,116,46,46,46), Color3.fromRGB(76, 175, 80))
        _0xStatusL.Text = string.char(83,116,97,116,117,115,58,32,65,99,116,105,118,101)
        _0xStatusL.TextColor3 = Color3.fromRGB(76, 175, 80)
        _0xStatusD.BackgroundColor3 = Color3.fromRGB(76, 175, 80)
        
        spawn(function()
            wait(1)
            pcall(function()
                loadstring(game:HttpGet(_0xPremScript))()
            end)
        end)
        
    else
        _0xNotify(string.char(73,110,118,97,108,105,100,32,107,101,121,33,32,80,108,101,97,115,101,32,116,114,121,32,97,103,97,105,110,46), Color3.fromRGB(244, 67, 54))
        _0xKeyI.Text = ""
    end
end)

