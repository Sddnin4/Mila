local function dd1(obj,info,props)
    local tw=a2:Create(obj,info,props)
    tw:Play()
    return tw 
end

local function ee1(msg,col)
    local nt=Instance.new("Frame")
    nt.Size=UDim2.new(0,300,0,50)
    nt.Position=UDim2.new(0.5,-150,0,-60)
    nt.BackgroundColor3=col or Color3.fromRGB(76,175,80)
    nt.Parent=e1 
    
    local nc=Instance.new("UICorner")
    nc.CornerRadius=UDim.new(0,8)
    nc.Parent=nt 
    
    local nt2=Instance.new("TextLabel")
    nt2.Size=UDim2.new(1,-10,1,0)
    nt2.Position=UDim2.new(0,5,0,0)
    nt2.BackgroundTransparency=1 
    nt2.Text=msg 
    nt2.TextColor3=Color3.fromRGB(255,255,255)
    nt2.TextScaled=true 
    nt2.Font=Enum.Font.Gotham 
    nt2.Parent=nt 
    
    dd1(nt,TweenInfo.new(0.3),{Position=UDim2.new(0.5,-150,0,20)})
    wait(3)
    dd1(nt,TweenInfo.new(0.3),{Position=UDim2.new(0.5,-150,0,-60)})
    wait(0.3)
    nt:Destroy()
end

local ff1,gg1,hh1,ii1,jj1
local kk1,ll1,mm1

ff1 = function()
    dd1(o1,TweenInfo.new(0.1),{Size=UDim2.new(0,115,0,32)})
    wait(0.1)
    dd1(o1,TweenInfo.new(0.1),{Size=UDim2.new(0,120,0,35)})
    
    local s,e = pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Sddnin4/Mila/refs/heads/main/Hub"))()
    end)
    
    if s then 
        ee1("Free Script Executed Successfully!",Color3.fromRGB(76,175,80))
    else 
        ee1("Failed to execute Free Script!",Color3.fromRGB(244,67,54))
    end 
end

gg1 = function()
    dd1(u1,TweenInfo.new(0.1),{Size=UDim2.new(0,145,0,32)})
    wait(0.1)
    dd1(u1,TweenInfo.new(0.1),{Size=UDim2.new(0,150,0,35)})
    
    local ik = s1.Text 
    local ck = "Hjk8dl"
    
    if ik == ck then 
        aa1.Text = "Status: Active"
        aa1.TextColor3 = Color3.fromRGB(76,175,80)
        bb1.BackgroundColor3 = Color3.fromRGB(76,175,80)
        ee1("Key Accepted! Loading...",Color3.fromRGB(76,175,80))
        
        local s,e = pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Sddnin4/Mila/refs/heads/main/Ui%20Loading"))()
        end)
        
        if s then 
            wait(1)
            e1:Destroy()
        else 
            ee1("Failed to load UI!",Color3.fromRGB(244,67,54))
        end 
    else 
        ee1("Invalid Key! Please try again.",Color3.fromRGB(244,67,54))
        s1.Text = ""
    end 
end

hh1 = function()
    dd1(w1,TweenInfo.new(0.1),{Size=UDim2.new(0,77,0,28)})
    wait(0.1)
    dd1(w1,TweenInfo.new(0.1),{Size=UDim2.new(0,80,0,30)})
    
    local ltc = "https://funlink.io/PaKX-wc"
    ee1("Get Key Link: "..ltc,Color3.fromRGB(33,150,243))
    
    if setclipboard then 
        setclipboard(ltc)
        ee1("Link copied to clipboard!",Color3.fromRGB(76,175,80))
    end 
end

ii1 = function()
    dd1(y1,TweenInfo.new(0.1),{Size=UDim2.new(0,28,0,28)})
    wait(0.1)
    dd1(y1,TweenInfo.new(0.1),{Size=UDim2.new(0,30,0,30)})
    ee1("Click 'Get Key' to get the access key!",Color3.fromRGB(255,193,7))
end

jj1 = function()
    dd1(f1,TweenInfo.new(0.3),{Size=UDim2.new(0,0,0,0),Position=UDim2.new(0.5,0,0.5,0)})
    wait(0.3)
    e1:Destroy()
end

o1.MouseButton1Click:Connect(ff1)
u1.MouseButton1Click:Connect(gg1)
w1.MouseButton1Click:Connect(hh1)
y1.MouseButton1Click:Connect(ii1)
k1.MouseButton1Click:Connect(jj1)

kk1,ll1,mm1 = false,nil,nil 

h1.InputBegan:Connect(function(inp)
    if inp.UserInputType == Enum.UserInputType.MouseButton1 then 
        kk1 = true 
        ll1 = inp.Position 
        mm1 = f1.Position 
    end 
end)

a3.InputChanged:Connect(function(inp)
    if kk1 and inp.UserInputType == Enum.UserInputType.MouseMovement then 
        local dlt = inp.Position - ll1 
        f1.Position = UDim2.new(mm1.X.Scale,mm1.X.Offset + dlt.X,mm1.Y.Scale,mm1.Y.Offset + dlt.Y)
    end 
end)

a3.InputEnded:Connect(function(inp)
    if inp.UserInputType == Enum.UserInputType.MouseButton1 then 
        kk1 = false 
    end 
end)

f1.Size = UDim2.new(0,0,0,0)
f1.Position = UDim2.new(0.5,0,0.5,0)
dd1(f1,TweenInfo.new(0.5,Enum.EasingStyle.Back),{Size=UDim2.new(0,400,0,350),Position=UDim2.new(0.5,-200,0.5,-175)})