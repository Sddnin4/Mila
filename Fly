local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Tạo Frame cho UI
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0.4, 0, 0.3, 0)
mainFrame.Position = UDim2.new(0.3, 0, 0.35, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
mainFrame.BackgroundTransparency = 0.5
mainFrame.Parent = ScreenGui

-- Tạo Tab Home
local homeTabButton = Instance.new("TextButton")
homeTabButton.Text = "Home"
homeTabButton.Size = UDim2.new(0.2, 0, 0.1, 0)
homeTabButton.Position = UDim2.new(0, 0, 0, 0)
homeTabButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
homeTabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
homeTabButton.Parent = mainFrame

-- Tạo Tab Script
local scriptTabButton = Instance.new("TextButton")
scriptTabButton.Text = "Script"
scriptTabButton.Size = UDim2.new(0.2, 0, 0.1, 0)
scriptTabButton.Position = UDim2.new(0.2, 0, 0, 0)
scriptTabButton.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
scriptTabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
scriptTabButton.Parent = mainFrame

-- Tạo Tab Notifications
local notificationsTabButton = Instance.new("TextButton")
notificationsTabButton.Text = "Notifications"
notificationsTabButton.Size = UDim2.new(0.2, 0, 0.1, 0)
notificationsTabButton.Position = UDim2.new(0.4, 0, 0, 0)
notificationsTabButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
notificationsTabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
notificationsTabButton.Parent = mainFrame

-- Tạo Tab Other
local otherTabButton = Instance.new("TextButton")
otherTabButton.Text = "Other"
otherTabButton.Size = UDim2.new(0.2, 0, 0.1, 0)
otherTabButton.Position = UDim2.new(0.6, 0, 0, 0)
otherTabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
otherTabButton.TextColor3 = Color3.fromRGB(0, 0, 0)
otherTabButton.Parent = mainFrame

-- Thêm chức năng "Chạy" trong tab Home
local runButton = Instance.new("TextButton")
runButton.Text = "Run"
runButton.Size = UDim2.new(0.2, 0, 0.1, 0)
runButton.Position = UDim2.new(0, 0, 0.2, 0)
runButton.BackgroundColor3 = Color3.fromRGB(0, 255, 255)
runButton.TextColor3 = Color3.fromRGB(0, 0, 0)
runButton.Parent = mainFrame

runButton.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 50  -- Chạy nhanh
        end
    end
end)

-- Thêm chức năng "Teleport" trong tab Home
local teleportButton = Instance.new("TextButton")
teleportButton.Text = "Teleport"
teleportButton.Size = UDim2.new(0.2, 0, 0.1, 0)
teleportButton.Position = UDim2.new(0.2, 0, 0.2, 0)
teleportButton.BackgroundColor3 = Color3.fromRGB(255, 0, 255)
teleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
teleportButton.Parent = mainFrame

teleportButton.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local targetPlayer = game.Players:FindFirstChild("PlayerName")  -- Thay "PlayerName" bằng tên người chơi bạn muốn teleport tới.
    if targetPlayer and targetPlayer.Character then
        player.Character:SetPrimaryPartCFrame(targetPlayer.Character.PrimaryPart.CFrame)
    end
end)
