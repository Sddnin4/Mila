a2.InputBegan:Connect(function(v6,v7)
    if v7 then return end
    if v6.KeyCode==Enum.KeyCode.RightShift then
        if f1.Visible then
            f1.Visible=false
            j1.Visible=true
            q1.Visible=false
        else
            f1.Visible=true
            j1.Visible=false
        end
    end
end)

b1.CharacterAdded:Connect(function(v8)
    b2=v8
    b3=b2:WaitForChild("Humanoid")
    c1=false
    c2=false
    if c5 then
        c5:Disconnect()
        c5=nil
    end
    wait(1)
    b3.WalkSpeed=c3
    b3.JumpPower=c4
end)

