local function u1(scriptName)
    o4.Text="Loading " ..scriptName.. "..."
    o2.Visible=true
    
    local u2
    u2=a3.Heartbeat:Connect(function()
        if not o2.Visible then
            u2:Disconnect()
            return
        end
        o5.Rotation=o5.Rotation+5
    end)
    
    wait(2)
    o2.Visible=false
    if u2 then
        u2:Disconnect()
    end
end

local function u3()
    c1=not c1
    if c1 then
        local u4
        u4=a3.Stepped:Connect(function()
            if not c1 then
                u4:Disconnect()
                return
            end
            for _,u5 in pairs(b2:GetChildren()) do
                if u5:IsA("BasePart") then
                    u5.CanCollide=false
                end
            end
        end)
    else
        for _,u5 in pairs(b2:GetChildren()) do
            if u5:IsA("BasePart") and u5.Name~="HumanoidRootPart" then
                u5.CanCollide=true
            end
        end
    end
end

local function u6()
    c2=not c2
    if c2 then
        c5=a2.JumpRequest:Connect(function()
            if b2 and b2:FindFirstChild("Humanoid") then
                b2.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    else
        if c5 then
            c5:Disconnect()
            c5=nil
        end
    end
end

local function u7()
    workspace.CurrentCamera.CameraSubject=b2.Humanoid
end

local function u8()
    pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Vehicle%20Fly%20Gui"))()
    end)
end

