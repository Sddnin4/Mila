for v2,v3 in pairs(k3) do
    v3.button.MouseButton1Click:Connect(function()
        c6=v2

        for v4,v5 in pairs(k3) do
            if v4==v2 then
                v5.button.TextColor3=Color3.new(0.2,0.4,0.8)
                v5.underline.Visible=true
            else
                v5.button.TextColor3=Color3.new(0.5,0.5,0.5)
                v5.underline.Visible=false
            end
        end

        m2.Visible=v2=="Home"
        m3.Visible=v2=="Script"
        m4.Visible=v2=="Notifications"
        m5.Visible=v2=="Other"
    end)
end

