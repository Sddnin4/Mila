local function r1(name,text,position,callback,parent)
    parent=parent or m2
    
    local r2=Instance.new("TextButton")
    r2.Name=name
    r2.Size=UDim2.new(0,100,0,25)
    r2.Position=position
    r2.BackgroundColor3=Color3.new(0.85,0.9,1)
    r2.Text=text
    r2.TextColor3=Color3.new(0.2,0.3,0.6)
    r2.TextSize=10
    r2.Font=Enum.Font.Gotham
    r2.Parent=parent
    
    local r3=Instance.new("UICorner")
    r3.CornerRadius=UDim.new(0,5)
    r3.Parent=r2
    
    r2.MouseButton1Click:Connect(callback)
    
    r2.MouseEnter:Connect(function()
        r2.BackgroundColor3=Color3.new(0.8,0.85,1)
    end)
    
    r2.MouseLeave:Connect(function()
        r2.BackgroundColor3=Color3.new(0.85,0.9,1)
    end)
    
    return r2
end

